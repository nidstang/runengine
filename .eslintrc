{
    "parser": "babel-eslint",
    "extends": ["airbnb-base"],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-unused-vars": 0,
        "object-curly-newline": 0,
        "arrow-parens": 0,
        "indent": ["error", 4],
        "import/no-extraneous-dependencies": 0,
        "linebreak-style": 0,
        "class-methods-use-this": 0,
        "import/no-unresolved": 0
    }
}
